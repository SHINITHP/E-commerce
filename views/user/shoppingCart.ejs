<body>



    <!-- ***** Header Area Start ***** -->
    <%- include('../../views/user/navbar.ejs') %>
        <!-- ***** Header Area End ***** -->

        <!-- Shop Cart Section Begin -->
        <section class="shop-cart spad ">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="shop__cart__table">
                            <table>
                                <thead>
                                    <tr style="text-align: center;">
                                        <th>Product Name</th>
                                        <th>Price</th>
                                        <th>Quantity</th>
                                        <th>Total</th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody  style="text-align: center;">
                                    
                                    <tr>
                                        <td class="cart__product__item">
                                            <img src="assets/images/shop-cart/cp-4.jpg" alt="">
                                            <div class="cart__product__item__title">
                                                <h6>Cotton Shirt</h6>
                                                <div class="rating">
                                                    <i class="fa fa-star"></i>
                                                    <i class="fa fa-star"></i>
                                                    <i class="fa fa-star"></i>
                                                    <i class="fa fa-star"></i>
                                                    <i class="fa fa-star"></i>
                                                </div>
                                            </div>
                                        </td>
                                        <td class="cart__price">$ 55.0</td>
                                        <td class="cart__quantity">
                                            <div class="pro-qty">
                                                <input type="text" value="1">
                                            </div>
                                        </td>
                                        <td class="cart__total">$ 110.0</td>
                                        <td class="cart__close"><span><i class="fa-solid fa-xmark" style="color: #000000;"></i></span></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-6">
                        <div class="cart__btn">
                            <a href="#" style="border: 1px solid #aeaeae;">Continue Shopping</a>
                        </div>
                    </div>
                    
                </div>
                <div class="row">
                    <div class="col-lg-6">
                        <div class="discount__content">
                            <h6>Discount codes</h6>
                            <form action="#">
                                <input type="text" placeholder="Enter your coupon code">
                                <button type="submit" class="site-btn">Apply</button>
                            </form>
                        </div>
                    </div>
                    <div class="col-lg-4 offset-lg-2">
                        <div class="cart__total__procced">
                            <h6>Cart total</h6>
                            <ul>
                                <li>Subtotal <span>$ 750.0</span></li>
                                <li>Total <span>$ 750.0</span></li>
                            </ul>
                            <a href="#" class="primary-btn">Proceed to checkout</a>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- Shop Cart Section End -->

        <!-- Instagram Begin -->
        <div class="instagram">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-2 col-md-4 col-sm-4 p-0">
